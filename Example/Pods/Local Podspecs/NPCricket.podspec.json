{
  "name": "NPCricket",
  "version": "0.1.3",
  "summary": "Allow users to send feedback directly from your iOS app",
  "description": "                       Allow users to send feedback directly from your iOS app\n",
  "homepage": "https://github.com/nebspetrovic/NPCricket",
  "license": "MIT",
  "authors": {
    "Nebojsa Petrovic": "nebspetrovic@gmail.com"
  },
  "source": {
    "git": "https://github.com/nebspetrovic/NPCricket.git",
    "tag": "0.1.3"
  },
  "social_media_url": "https://twitter.com/nebsp",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": [
    "Pod/Classes/*.{h,m}",
    "Pod/Classes/Categories/*.{h,m}",
    "Pod/Classes/Handlers/*.h",
    "Pod/Classes/Models/*.{h,m}",
    "Pod/Classes/Views/*.{h,m}"
  ],
  "resource_bundles": {
    "NPCricket": [
      "Pod/Classes/**/*.xib"
    ]
  },
  "public_header_files": "Pod/Classes/**/*.h",
  "subspecs": [
    {
      "name": "NativeEmail",
      "source_files": "Pod/Classes/Handlers/NativeEmail/*.{h,m}"
    }
  ]
}
